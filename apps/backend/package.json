  "scripts": {
    "build": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch-metadata": "ts-node ./generate-metadata.ts --watch",
    "prebuild": "rimraf dist",
    "start": "concurrently  --raw  \"pnpm run watch-metadata\" \"pnpm run serve-dev\" --kill-others --success first",
    "serve-dev": "pnpm run prebuild && pnpm run with-op-env webpack --config webpack.config.js --watch",
    "start:prod": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "with-op-env": "NODE_ENV=${PROJECT} ts-node ./src/prep-env-vars.ts -v -- ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "obfuscate": "javascript-obfuscator headInjection.min.js --output public/hpi.min.js --config obfuscator-config.json",
    "build-head-injection:dev": "npm run minify:dev && npm run obfuscate",
    "build-head-injection:prod": "npm run minify:prod && npm run obfuscate"
  },
